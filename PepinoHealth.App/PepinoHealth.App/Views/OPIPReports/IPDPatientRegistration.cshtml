
@{
    ViewBag.Title = "IPDPatientRegistration";
}

<script type="text/javascript">
    $(document).ready(function () {
        applyDatePicker(
            'txtStartDateTime',
            'txtEndDateTime',
            calendarFormatType.Date,
            function (e, date) {
            });
        hideModal('loader');
    });
</script>

<div class="p-block v2">
    <div class="p-header">
        <div class="title">
            IPD Patient Registration Reports
        </div>
    </div>
    <div class="p-body">
        <div class="p-body-content">
            <form id="formDebitDetails">
                <section>

                    <div class="body">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>DEPARTMENT:</label>
                                            <div class="input-group">
                                                <select id="ddlIPDDepartment" name="ddlIPDDepartment" class="custom-select my-1 mr-sm-2 labelsm">
                                                    <option value="ALL" selected>ALL</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Start Date:</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" id="txtStartDateTime" name="txtStartDateTime" />
                                                <button class="btn btn-info btn-sm btn-search-icon" type="button">
                                                    <i class="material-icons">date_range</i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>END Date:</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" id="txtEndDateTime" name="txtEndDateTime" />
                                                <button class="btn btn-info btn-sm btn-search-icon" type="button">
                                                    <i class="material-icons">date_range</i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label></label>
                                        <div class="input-group">
                                            <button type="button" class="btn btn-success btn-sm">
                                                Save
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="row">
                                    <div class="col-xs-12 col-md-12 col-sm-12 col-lg-12" id="tblipdregistration">
                                        <div class="page-title" style="text-align:center">
                                            <h5 class="text-uppercase pagetitle">NEW VISIT PATIENT DETAILS</h5>
                                        </div>
                                       
                                        <table class="table table-striped table-bordered nowrap t-voucher" cellspacing="0" width="100%" id="tbl-ipdregistration">
                                            <thead>
                                                <tr>
                                                    <th>Number</th>
                                                    <th>Date</th>
                                                    <th>Year</th>
                                                    <th>PrefixName</th>
                                                    <th>Name</th>
                                                    <th>FatherName</th>
                                                    <th>Husband</th>
                                                    <th>Address</th>
                                                    <th>Gender</th>
                                                    <th>Age</th>
                                                    <th>Department</th>
                                                    <th>Doctor</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </form>
        </div>
    </div>
</div>

